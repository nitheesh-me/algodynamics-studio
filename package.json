{
  "name": "lgodynamics-studio",
  "version": "0.0.2",
  "private": true,
  "scripts": {
    "dev": "astro dev --host 0.0.0.0 --port 4444",
    "start": "astro dev",
    "build": "astro check && astro build",
    "check": "astro check",
    "preview": "astro preview",
    "astro": "astro",
    "format": "bun run format:code",
    "format:ci": "bun run format:imports && bun run format:code",
    "format:code": "prettier -w . --cache --plugin-search-dir=.",
    "format:imports": "echo 'Skipped formatting imports'",
    "docgen": "bun ./scripts/docgen.mjs",
    "docgen:errors": "bun ./scripts/error-docgen.mjs",
    "lint:linkcheck": "SKIP_OG=true bun build && tsm --require=./scripts/lib/filter-warnings.cjs ./scripts/lint-linkcheck.ts",
    "lint:linkcheck:nobuild": "tsm --require=./scripts/lib/filter-warnings.cjs ./scripts/lint-linkcheck.ts",
    "lint:slugcheck": "bun ./scripts/lint-slugcheck.mjs",
    "lint:eslint": "eslint .",
    "lunaria:build": "tsm ./scripts/lunaria.mts"
  },
  "devDependencies": {
    "@11ty/eleventy-fetch": "^5.0.1",
    "@actions/core": "^1.11.1",
    "@docsearch/css": "3.5.1",
    "@docsearch/js": "3.5.1",
    "@eslint/js": "^9.17.0",
    "@types/canvas-confetti": "^1.6.0",
    "@types/hast": "^3.0.4",
    "@types/html-escaper": "^3.0.2",
    "@types/mdast": "^4.0.4",
    "@types/node": "^20.17.10",
    "@typescript-eslint/parser": "^8.19.1",
    "astro": "^5.3.0",
    "astro-auto-import": "^0.4.4",
    "astro-eslint-parser": "^1.1.0",
    "astro-og-canvas": "^0.5.6",
    "canvaskit-wasm": "^0.39.1",
    "dedent-js": "^1.0.1",
    "domhandler": "^5.0.3",
    "eslint": "^9.17.0",
    "eslint-plugin-astro": "^1.3.1",
    "fast-glob": "^3.3.3",
    "globals": "^15.14.0",
    "hast-util-select": "^6.0.3",
    "hast-util-to-string": "^3.0.1",
    "hastscript": "^9.0.0",
    "html-escaper": "^3.0.3",
    "htmlparser2": "^9.1.0",
    "kleur": "^4.1.5",
    "mdast-util-to-string": "^4.0.0",
    "p-retry": "^6.2.1",
    "prettier": "^3.4.2",
    "prettier-plugin-astro": "^0.14.1",
    "rehype": "^13.0.2",
    "tsm": "^2.3.0",
    "typescript": "5.6.2",
    "typescript-eslint": "^8.19.1",
    "unified": "^11.0.5",
    "unist-util-visit": "^5.0.0"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/sitemap": "^3.2.1",
    "@astrojs/starlight": "^0.31.1",
    "@expressive-code/plugin-collapsible-sections": "^0.40.2",
    "@lunariajs/core": "https://pkg.pr.new/lunariajs/lunaria/@lunariajs/core@4c8b9b0",
    "canvas-confetti": "^1.6.0",
    "jsdoc-api": "^9.3.4",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-slug": "^6.0.0",
    "remark-gfm": "^4.0.0",
    "remark-smartypants": "^2.0.0",
    "sharp": "^0.33.5"
  },
  "packageManager": "bun@1.2.2"
}
